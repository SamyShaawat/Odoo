<odoo>
    <record model="ir.ui.view" id="quick_transations_formview">
        <field name="name">quick.transations.form</field>
        <field name="model">quick.transations</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="date"/>
                    <field name="expense_type"/>
                    <field name="name"/>
                    <field name="amount" sum="total"/>
                    <field name="stage" readonly="1"/>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="quick_transations_treeview">
        <field name="name">quick.transations.tree</field>
        <field name="model">quick.transations</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="date"/>
                <field name="expense_type"/>
                <field name="name"/>
                <field name="amount"/>
                <field name="stage" readonly="1"/>
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="expense_type_formview">
        <field name="name">expense.type.form</field>
        <field name="model">expense.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="expense_name"/>
                    <field name="account_id"/>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="expense_type_treeview">
        <field name="name">expense.type.tree</field>
        <field name="model">expense.type</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="expense_name"/>
                <field name="account_id"/>
            </tree>
        </field>
    </record>


    <record id="quick_transactions_action" model="ir.actions.act_window">
        <field name="name">Quick Transactions</field>
        <field name="res_model">quick.transations</field>
        <field name="view_mode">tree,form</field>
        <field name="context"></field>
    </record>
    <record id="expense_type_action" model="ir.actions.act_window">
        <field name="name">Expense Type</field>
        <field name="res_model">expense.type</field>
        <field name="view_mode">tree,form</field>
        <field name="context"></field>
    </record>

    <record model="ir.actions.server" id="server_post_transations">
        <field name="name">Post Transactions</field>
        <field name="model_id" ref="model_quick_transations"/>
        <field name="binding_model_id" ref="model_quick_transations"/>
        <field name="state">code</field>
        <field name="code">action = model.post_transactions()</field>
    </record>

    <menuitem id="quick_transactions_menu" name="Quick Transactions" action="quick_transactions_action"
              parent="account_accountant.menu_accounting" sequence="40"/>

    <menuitem id="expense_type_menu" name="Expense Type" action="expense_type_action"
              parent="account_accountant.menu_accounting" sequence="50"/>
</odoo>