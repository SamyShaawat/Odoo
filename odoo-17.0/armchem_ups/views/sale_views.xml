<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="armchem_sale_order_upswsout_form" model="ir.ui.view">
            <field name="name">armchem.sale.order.upswsout.form.inherit</field>
            <field name="model">sale.order</field>
            <field name="priority">5</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook[last()]" position="inside">
                    <page string="Tracking" name="upswsout">
                        <field name="upsws_out_ids" readonly="1">
                            <tree>
                                <field name="shipmentinformationcollectiondate" string="Date"/>
                                <field name="packagetrackingnumber" string="Tracking Number"/>
                            </tree>
                            <form>
                                <group>
                                    <field name="shipmentinformationcollectiondate" string="Date"/>
                                    <field name="packagetrackingnumber" string="Tracking Number"/>
                                </group>
                            </form>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>

        <record id="inherit_sale_order_search_view" model="ir.ui.view">
            <field name="name">inherit_sale_order_search_view</field>
            <field name="model">sale.order</field>
            <field name="priority">5</field>
            <field name="inherit_id" ref="sale.sale_order_view_search_inherit_quotation"/>
            <field name="arch" type="xml">
                <xpath expr="//search" position="inside">
                    <field name="pricelist_id"/>
                    <field name="payment_term_id"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>