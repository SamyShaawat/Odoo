<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="web_upswsout_form" model="ir.ui.view">
        <field name="name">web.upsws.out.form</field>
        <field name="model">web.upsws.out</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="packagereference3"/>
                    <field name="packagereference2"/>
                    <field name="shipmentinformationcollectiondate"/>
                    <field name="packagetrackingnumber"/>
                    <field name="packagepackagepublishedcharge"/>
                    <field name="shipmentinformationvoidindicator"/>
                    <field name="shipmentinformationhundredweight"/>
                    <field name="shipmentinformationtotalshipmentpublishedcharge"/>
                </group>
            </form>
        </field>
    </record>

    <record id="web_upswsout_tree" model="ir.ui.view">
        <field name="name">web.upsws.out.tree</field>
        <field name="model">web.upsws.out</field>
        <field eval="100" name="priority"/>
        <field name="arch" type="xml">
            <tree>
                <field name="packagereference3"/>
                <field name="packagereference2"/>
                <field name="shipmentinformationcollectiondate"/>
                <field name="packagetrackingnumber"/>
                <field name="packagepackagepublishedcharge"/>
                <field name="shipmentinformationvoidindicator"/>
                <field name="shipmentinformationhundredweight"/>
                <field name="shipmentinformationtotalshipmentpublishedcharge"/>
            </tree>
        </field>
    </record>

    <record id="web_upswsout_filter" model="ir.ui.view">
        <field name="name">web.upsws.out.select</field>
        <field name="model">web.upsws.out</field>
        <field name="arch" type="xml">
            <search string="Search Move">
                <field name="packagereference3"/>
                <field name="packagereference2"/>
                <field name="shipmentinformationcollectiondate"/>
                <field name="packagetrackingnumber"/>
                <field name="packagepackagepublishedcharge"/>
                <field name="shipmentinformationvoidindicator"/>
                <field name="shipmentinformationhundredweight"/>
                <field name="shipmentinformationtotalshipmentpublishedcharge"/>
            </search>
        </field>
    </record>

    <record id="action_web_upswsout" model="ir.actions.act_window">
        <field name="name">UPSWSOUT</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">web.upsws.out</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
