<odoo>
    <record id="view_res_company_form_inherit" model="ir.ui.view">
        <field name="name">res.company.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
             <xpath expr="//form/sheet" position="inside">
                <group string="Commission Accounts">
                    <field name="commission_percentage" widget="percentage"/>
                    <field name="commission_liability_account_id"/>
                    <field name="commission_payable_account_id"/>
                    <field name="commission_expense_account_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
