<?xml version="1.0"?>
<odoo>
    <data>
        <!-- Form View -->
        <record id="clinic_view" model="ir.ui.view">
            <field name="name">Clinics</field>
            <field name="model">clinic.model</field>
            <field name="priority" eval="15"/>
            <field name="arch" type="xml">
                <form string="Cleopatra Clinics" class="register-form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="assigned_to" widget="many2many_tags"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Tree View -->
        <record model="ir.ui.view" id="ai_assistant_tree_view">
            <field name="name">Clinics</field>
            <field name="model">clinic.model</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <!-- Action -->
        <record id="clinic_action" model="ir.actions.act_window">
            <field name="name">Cleopatra Clinics</field>
            <field name="res_model">clinic.model</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>
