<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="payment_provider_webhook_form" model="ir.ui.view">
            <field name="name">Provider.mastercard.mpgs.webhook</field>
            <field name="model">payment.provider</field>
          <field name="inherit_id" ref="aspl_payment_mpgs_ee.payment_provider_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='mpgs_secret_key']" position="after">
                <field name="mpgs_notification_secret" password="True" />
            </xpath>

        </field>
        </record>
    </data>
</odoo>
