<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <t t-name="commission_reports.commission_end_of_year_buttons">
        <button type="button" class='btn btn-primary js_report_commission_end_of_year_pdf'>Pdf</button>
        <button type="button" class='btn btn-primary js_report_commission_end_of_year_xlsx'>Excel</button>
    </t>

    <form class="form-row" t-name="commission_reports.commission_end_of_year_searchview_buttons">
        <label class="custom-date mr-1" for="date_from">Start :</label>
        <div class="form-group mr-2">
            <div class='o_datepicker js_commission_reports_datetimepicker'
                 t-att-data-default-value="options['date_from']">
                <input type='text' class="o_datepicker_input" name="date_from"/>
                <span class="o_datepicker_button"/>
            </div>
        </div>

        <label class="custom-date mr-1" for="date_to">End :</label>
        <div class="form-group">
            <div class='o_datepicker js_commission_reports_datetimepicker'
                 t-att-data-default-value="options['date_to']">
                <input type='text' class="o_datepicker_input" name="date_to"/>
                <span class="o_datepicker_button"/>
            </div>
        </div>

        <button type="button" class="btn btn-secondary js_commission_report_search_btn" data-filter="custom">
            <i class="fa fa-search"/>
        </button>
    </form>

</templates>